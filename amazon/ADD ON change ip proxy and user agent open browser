from pip._internal.network.session import user_agent
from selenium import webdriver






class AliExpressScraper:

    def __init__(self, start):
        print("-----------------INITIALIZING SCRAPER--------------\n")

PROXY = " "
PROXY = input("PLEASE ENTER Proxy IP & Wait:      ")
webdriver.DesiredCapabilities.CHROME['proxy'] = {
    "httpProxy": PROXY,
    "ftpProxy": PROXY,
    "sslProxy": PROXY,
    "noProxy": None,
    "proxyType": "MANUAL",
    "autodetect": False
}
print(PROXY)



def main():


    start = str(print("-----------------INITIALIZING SCRAPER ?--------------\n") )



def verify(key):
    print(key)
    score = 0
    check_digit = key[2]
    check_digit_count = 0
    chunks = key.split('-')
    for chunk in chunks:
        if len(chunk) != 4:
            return False
        for char in chunk:
            if char == check_digit:
                check_digit_count += 1
            score += ord(char)
    if score == 1672 and check_digit_count == 5:
        return True
    return False


def validate_user():
    while True:
        key = input("PLEASE ENTER A VALID KEY TO RUN THE PROGRAM:  ")
        try:
            if verify(key.lower()):
                print("KEY VALID:")
                return True
            else:
                print("INVALID KEY, PLEASE ENTER A VALID KEY TO RUN THE PROGRAM\n")
        except IndexError:
            print("PLEASE ENTER A VALID KEY\n")


# strings in here on main function

if __name__ == "__main__":
    if validate_user():
        main()
        if user_agent():
            main()


from selenium import webdriver
from selenium.webdriver.chrome.options import Options
from webdriver_manager.chrome import ChromeDriverManager
import time
driver = webdriver.Chrome(ChromeDriverManager().install())

user_agent = " "
user_agent = input("PLEASE ENTER User Agent & Wait:      ")

opts = Options()
opts.add_argument("user-agent=" + user_agent)
driver = webdriver.Chrome(options=opts)
String_userAgent = driver.execute_script("return navigator.userAgent")
print(String_userAgent)
time.sleep(10)
driver.get('https://www.whatsmyip.org/')
time.sleep(10)
print(PROXY)


